#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>

char notated_double_str[100];
char result[40];

int TO_DIGIT(char symb) {
	int digit;
	if (symb <= '9') {
		digit = symb - '0';
	}
	else {
		digit = symb + 10 - 'A';
	}
	return digit;
}

int TO_SYMBOL(int digit_to_symb) {
	char symb;
	if (digit_to_symb <= 9) {
		symb = digit_to_symb + '0';
	}
	else {
		symb = digit_to_symb - 10 + 'A';
	}
	return symb;
}

double TO_DEC(int digi, int intpower, int b1sys) {
	double powered = 0;
	powered = digi*(pow(b1sys, intpower));
	return powered;
}

void TO_NOTATION_INT(long long int dec_int, int notation) {
	char notated_int_str[60], reversed_notated_int_str[60];
	int remaind, i, total;
	if (dec_int > 0) {
		for (i = 0; dec_int > 0; i++) {
			remaind = dec_int % notation;
			notated_int_str[i] = TO_SYMBOL(remaind);
			notated_int_str[i + 1] = '\0';
			dec_int = dec_int / notation;
		}
		total = strlen(notated_int_str);
		for (int k = total-1, j = 0; k >= 0; k--) {
			reversed_notated_int_str[j] = notated_int_str[k];
			j++;
		}
		reversed_notated_int_str[total] = '\0';
		strcpy(result, reversed_notated_int_str);
	}
	else {
		sprintf(result, "%lld", dec_int);
	}
}

void TO_NOTATION_DOUBLE(int dec_double, int notation) {
	char test [100];
	int digit_counter, max_float_digitals = 0, decade = 1, length = 0, x = 0;
	unsigned int l = 0;
	sprintf(notated_double_str, "%d", dec_double);
	digit_counter = strlen(notated_double_str);
	for (int i = 1; i <= digit_counter; i++) {
		decade *= 10;
	}
	while ((dec_double != 0) && (l < 12)) {
		dec_double *= notation;
		if (dec_double >= decade) {
			sprintf(test, "%d", (dec_double / decade));
			length = l;
			for (l; l < (strlen(test)+length); l++) {
				notated_double_str[l] = test[0+x];
				notated_double_str[l + 1] = '\0';
				if (l == 11) {
					break;
				}
				x++;
			}
			dec_double = dec_double % decade;
			x = 0;
		}
		else {
			sprintf(test, "%d", dec_double / decade);
			notated_double_str[l] = test[0];
			notated_double_str[l + 1] = '\0';
			l++;
		}
	}
}

int main() {
	char b1instr[3], b2instr[3], valinstr[14], dec_val_str[60], dec_val_str_double [50];
	int b1, b2, dig = 0, power = 0, dot = 0, dot_dec = 0;
	long long int val_dec_int = 0;
	double val_dec_double = 0.;
	scanf("%s%s%s", b1instr, b2instr, valinstr);
	b1 = atoi(b1instr);
	b2 = atoi(b2instr);
	if (((b1 < 2) || (b1 > 16)) || ((b2 < 2) || (b2 > 16))) {
		printf("bad input\n");
	}
	else {
		for (unsigned int i = 0; i < strlen(valinstr); i++) {
			if (TO_DIGIT(valinstr[i]) >= b1) {
				printf("bad input\n");
				exit(0);
			}
			else {
				if (valinstr[i] == '.') {
					dot = i;
				}
			}
		}
		if (dot > 0) {
			power = dot - 1;
			for (unsigned int i = 0; i < strlen(valinstr); i++) {
				if ((i == dot) && (i>0)) {
					continue;
				}
				else {
					dig = TO_DIGIT(valinstr[i]);
					val_dec_double += TO_DEC(dig, power, b1);
					power--;
				}
			}
			sprintf(dec_val_str, "%g", val_dec_double);
			if (b2 != 10) {
				for (unsigned int i = 0; i < strlen(dec_val_str); i++) {
					if (dec_val_str[i] == '.') {
						dot_dec = i;
					}
				}
				if (dot_dec > 0) {
					for (unsigned int i = 0; i < strlen(dec_val_str) - dot_dec; i++) {
						dec_val_str_double[i] = dec_val_str[dot_dec + i + 1];
						dec_val_str_double[i + 1] = '\0';
					}
					val_dec_int = atoi(dec_val_str_double);
					TO_NOTATION_DOUBLE((int)val_dec_int, b2);
					dec_val_str[dot_dec] = '\0';
					val_dec_int = atoi(dec_val_str);
					TO_NOTATION_INT(val_dec_int, b2);
					strcat(result, ".");
					strcat(result, notated_double_str);
				}
				else {
					val_dec_int = long long int(val_dec_double);
					TO_NOTATION_INT(val_dec_int, b2);
				}
			}
			else {
				strcpy(result, dec_val_str);
			}
		}
		else {
			power = strlen(valinstr) - 1;
			for (unsigned int i = 0; i < strlen(valinstr); i++) {
				dig = TO_DIGIT(valinstr[i]);
				val_dec_int += long long int(TO_DEC(dig, power, b1));
				power--;
			}
			TO_NOTATION_INT(val_dec_int, b2);
		}
		printf("%s\n", result);
	}
	return 0;
}
